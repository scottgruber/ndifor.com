{# Pull Quote Block Twig Template Partial #}

{% if block.pullQuote|length %}

{% switch block.position %}

{% case "normal" %}
<blockquote>
{{ block.pullQuote }}
</blockquote>

{% case "inlineStart" %}
<blockquote class="c-pullquote--inline-start">
{{ block.pullQuote }}
</blockquote>

{% case "inlineEnd" %}
<blockquote class="c-pullquote--inline-end">
{{ block.pullQuote }}
</blockquote>

{% endswitch %}


{% endif %}